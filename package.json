{
  "name": "aframe-go",
  "version": "1.0.0",
  "description": "Aframe starterkit",
  "repository": "git@github.com:shaundunne/aframe-go.git",
  "author": "Shaun Dunne <shaun@studiozero.co>",
  "license": "MIT",
  "scripts": {
    "start": "npm run start-pug | npm run start-budo",
    "start-pug": "pug src/*.pug --watch",
    "start-budo": "budo src/assets/js/main.js:src/assets/js/bundle.js --serve assets/js/bundle.js --dir ./src/ --live --verbose --port 3000 -- -t [ babelify --presets [ es2015 ] ]",
    "build": "npm run build-pug && npm run build-js && rm -rf dist && mkdir dist && cp src/index.html dist && cp -r src/assets dist",
    "build-pug": "pug src/*.pug",
    "build-js": "browserify src/assets/js/main.js -o src/assets/js/bundl
    .js -- -t [ babelify --presets [ es2015 ] ]",
    "deploy:upload": "surge -p dist -d {YOUR.SURGE.SH DOMAIN}",
    "deploy": "npm run build && npm run deploy:upload"
  },
  "dependencies": {
    "aframe": "^0.4.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^14.0.0",
    "budo": "^9.4.7",
    "pug": "^2.0.0-beta10",
    "pug-cli": "^1.0.0-alpha6"
  }
}
